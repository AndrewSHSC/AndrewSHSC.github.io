<!DOCTYPE html>
<html class="no-js" lang="en" itemscope itemtype="http://schema.org/WebPage">
  <head>
    <title>BMT</title>
    <!--@import _structure/_meta-->
  </head>
  <body>
    <!--@import _structure/header-->
    <!--@import _structure/mobilenav-->

    <!-- Height is defined by image. To increase size just amend the image dimensions -->
    <!-- fix text centering when no breadcrumb on page by adding 'no-breadcrumb' class -->

    <section class="m-pagebanner dark-overlay no-breadcrumb">
      <div class="e-image">
        <picture>
          <!--[if IE 9]><video style="display: none;"><![endif]-->
          <source srcset="images/partials/pagebanner/banner-1920x650.jpg" media="(min-width: 1630px)">
          <source srcset="images/partials/pagebanner/banner-1630x650.jpg" media="(min-width: 1460px)">
          <source srcset="images/partials/pagebanner/banner-1430x650.jpg" media="(min-width: 1200px)">
          <source srcset="images/partials/pagebanner/banner-1200x600.jpg" media="(min-width: 991px)">
          <source srcset="images/partials/pagebanner/banner-991x600.jpg" media="(min-width: 767px)">
          <source srcset="images/partials/pagebanner/banner-767x650.jpg" media="(min-width: 500px)">
          <source srcset="images/partials/pagebanner/banner-500x600.jpg" media="(min-width: 0px)">
          <!--[if IE 9]></video><![endif]-->
          <img src="images/partials/pagebanner/banner-1920x650.jpg" alt="...">
        </picture>
      </div>
      <div class="container">
        <div class="e-text">
          <h1>Banner title</h1>
          <p class='preamble'>Unlock smarter ways to extract, process and handle commodities. We help you tackle themost challenging problems.</p>
        </div>
      </div>
    </section>


    <!-- add 'background-grey' to 'm-filters' to change filtertop background to grey -->
    <!-- requires filters.js -->

    <section class="m-filters">
      <p class='filter-mob-toggle'>Search and filter</p>

      <div class="e-filtercontainer">
        <div class="m-filtertop">
          <div class="container">
            <div class="e-filter filter-by">
              <div>
                <p>Filter By:</p>
              </div>
            </div>
            <div class="e-filter select">
              <select name="" id="">
                <option value="Option 1" disabled selected>Market</option>
                <option value="Option 2">Option 2</option>
                <option value="Option 3">Option 3</option>
              </select>
            </div>
            <div class="e-filter select">
              <select name="" id="">
                <option value="Option 1" disabled selected>Region</option>
                <option value="Option 2">Option 2</option>
                <option value="Option 3">Option 3</option>
              </select>
            </div>
            <div class="e-filter search">
              <input type="text" name="" id="filterSearch" placeholder="Search by keyword">
              <button>
                <svg class="icon icon-icon_search">
                  <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="images/icons.svg#icon-icon_search"></use>
                </svg>
              </button>
            </div>
            <div class="e-filter button">
              <button type="submit" class='btn'>
                Search
              </button>
            </div>
          </div>
        </div>

        <div class="m-filterbottom">
          <div class="container">
            <ul>
              <li class="e-currentfilters"><span>Current filters:</span></li>
              <li>
                <a href="" class="e-tag">Option 1 <span>(34)</span></a>
              </li>
              <li>
                <a href="" class="e-tag">Option 2 <span>(18)</span></a>
              </li>
              <li>
                <a href="" class="e-tag">Option 3 <span>(4)</span></a>
              </li>
            </ul>
            <a href="" class="e-clearfilters">Clear all</a>
          </div>
        </div>

